{
  "name": "es6-starter",
  "version": "1.0.0",
  "description": "Basic skeleton codebase for ES6 development",
  "scripts": {
    "preinstall": "npm set audit false",
    "start": "webpack-dev-server --config webpack/dev.config.js --watch",
    "build": "cross-env NODE_ENV=production webpack --config webpack/prod.config.js --progress --display-error-details --color",
    "heroku-postbuild": "npm run build",
    "precommit": "eslint src",
    "prod": "NODE_ENV=production node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:bottega-code-school/js-builder.git"
  },
  "keywords": [
    "es6",
    "es7",
    "webpack2",
    "webpack3",
    "starter",
    "template"
  ],
  "author": "Your Name Here",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bottega-code-school/js-builder/issues"
  },
  "homepage": "https://github.com/bottega-code-school/js-builder",
  "devDependencies": {
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack-dev-server": "^3.1.5",
    "webpack": "^4.16.4",
    "webpack-dev-middleware": "^3.1.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0"
  },
  "dependencies": {
    "autoprefixer": "^9.1.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.1.18",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.3.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-react": "^7.1.0",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "moment": "^2.24.0",
    "url-loader": "^1.0.1",
    "webpack-cli": "^3.1.0",
    "webpack-merge": "^4.1.0"
  }
}
